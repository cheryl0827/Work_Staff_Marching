            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/urgent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="3dp">
        <TextView
            android:id="@+id/qggrggr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:text="心理抗压能力："
            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/psychology"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/ggar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="39dp"
            android:text="组织协调能力："
            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/organization"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="3dp">
        <TextView
            android:id="@+id/qggr1ggr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="32dp"
            android:text="分析研判能力："
            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/analyse"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/workuserNoggar1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="39dp"
            android:text="依法办事能力："
            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/law"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>
</LinearLayout>
    <LinearLayout
        android:id="@+id/opetation"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/audit"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:text="审核"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="330dp"
            android:background="@color/mediumpurple"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/opetation1"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/audit1"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:text="重新审核"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="330dp"
            android:background="@color/mediumpurple"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/opetation2"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/audit2"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:text="审核"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="330dp"
            android:background="@color/mediumpurple"/>
    </LinearLayout>
    <LinearLayout
        android:id="@+id/opetation3"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/audit3"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:text="重新审核"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="330dp"
            android:background="@color/mediumpurple"/>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/workEvaluatingIndicator"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/updateworkEvaluatingIndicator"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:text="修改指标值"
            android:layout_marginLeft="215dp"
            android:layout_marginTop="5dp"
            android:background="@color/mediumpurple"/>
        <Button
            android:id="@+id/add"
            android:layout_width="80dp"
            android:layout_height="30dp"
            android:text="任务数操作"
            android:layout_marginTop="5dp"
            android:layout_marginLeft="25dp"
            android:background="@drawable/setbutton"/>
    </LinearLayout>



    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/thistle"
        />




