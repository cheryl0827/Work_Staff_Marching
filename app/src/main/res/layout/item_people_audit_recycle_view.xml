<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="wrap_content"
    android:background="#fff">

    <LinearLayout
        android:id="@+id/adressLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="2dp">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="27dp"
            android:layout_height="23dp"
            android:src="@mipmap/phone" />

        <TextView
            android:id="@+id/userName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:text="张三"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:paddingLeft="40dp"
            android:text="手机号码："
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13457898653"
            android:textColor="@color/black"
            android:textSize="18dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/work"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="4dp">

        <TextView
            android:id="@+id/workuserNoText"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="28dp"
            android:text="工号："
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/workuserNo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="1342432221"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:text="性别："
        android:textColor="@color/black"
        android:textSize="18dp" />

        <TextView
            android:id="@+id/sex"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="女"
            android:textColor="@color/black"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/textView130"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:text="max任务数："
            android:visibility="gone"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/max"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="35"
            android:visibility="gone"
            android:textColor="@color/black"
            android:textSize="18dp" />


    </LinearLayout>
    <LinearLayout
        android:id="@+id/user"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="4dp">

        <TextView
            android:id="@+id/indentificationCard1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="38dp"
            android:text="身份证号："
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/identification"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="13553385352"
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/sds122"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="37dp"
            android:text="性别："
            android:textColor="@color/black"
            android:textSize="18dp" />

        <TextView
            android:id="@+id/sexuser"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="女"
            android:textColor="@color/black"
            android:textSize="18dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        android:id="@+id/addressLinearLayout">
        <ImageView
            android:id="@+id/imageView113"
            android:layout_width="27dp"
            android:layout_height="23dp"
            android:src="@mipmap/address" />

        <TextView
            android:id="@+id/address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:paddingLeft="3dp"
            android:text="福建省-福州市-闽侯县-上街镇福建江夏学院"
            android:textColor="@color/black"
            android:textSize="18dp"
            android:layout_marginLeft="3dp"/>
    </LinearLayout>
<LinearLayout
