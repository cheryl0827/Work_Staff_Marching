            android:textColor="@color/black"
            android:textSize="18dp"
            />
        <TextView
            android:id="@+id/community"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:textColor="@color/black"
            android:textSize="18dp" />
        <TextView
            android:id="@+id/ggar"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
