<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    android:orientation="vertical">
        <com.qmuiteam.qmui.widget.QMUIRadiusImageView
            android:id="@+id/iv_upload_image"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:src="@mipmap/me1"
            android:layout_gravity="center"
            />

    <LinearLayout
        android:paddingTop="30dp"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">
        <TextView
            android:id="@+id/textView1116"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="沟通亲和能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/community"
            android:layout_width="30dp"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:layout_weight="1"
            android:layout_marginLeft="20dp"
            android:textSize="22dp"
            android:inputType="number"
            android:text=""/>
        <TextView
            android:id="@+id/community1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">
        <TextView
            android:id="@+id/textView1016"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="应急处置能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/urgent"
            android:layout_width="30dp"
            android:inputType="number"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:textSize="22dp"
            android:layout_marginLeft="20dp"
            android:text=""/>
        <TextView
            android:id="@+id/urgent1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">
        <TextView
            android:id="@+id/textView8116"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:text="心理抗压能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/psychology"
            android:layout_width="30dp"
            android:layout_weight="1"
            android:inputType="number"
            android:layout_marginLeft="20dp"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:textSize="22dp"
            android:text=""/>
        <TextView
            android:id="@+id/psychology1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">

        <TextView
            android:id="@+id/textView116"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="组织协调能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/organization"
            android:layout_width="30dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:inputType="number"
            android:layout_marginLeft="20dp"
            android:textSize="22dp"
            android:text=""/>
        <TextView
            android:id="@+id/organization1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">
        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="分析研判能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/analyse"
            android:layout_width="30dp"
            android:layout_weight="1"
            android:inputType="number"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:layout_marginLeft="20dp"
            android:textSize="22dp"
            android:text=""/>
        <TextView
            android:id="@+id/analyse1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginLeft="25dp">
        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="依法办事能力:"
            android:textColor="#808080"
            android:textSize="22dp" />
        <EditText
            android:id="@+id/law"
            android:layout_width="30dp"
            android:layout_weight="1"
            android:layout_marginLeft="20dp"
            android:inputType="number"
            android:layout_height="wrap_content"
            android:textColor="#808080"
            android:textSize="22dp"
            android:text=""/>
        <TextView
            android:id="@+id/law1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:textColor="@color/red"
            android:text="*"
            android:textSize="20dp" />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#DCDCDC"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="88dp"
        android:layout_marginLeft="90dp"
        android:layout_marginTop="50dp"
        android:orientation="horizontal">

        <Button
            android:id="@+id/ok"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/setbutton"
            android:text="完成" />

        <Button
            android:id="@+id/cancel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="50dp"
            android:background="@drawable/setbutton"
            android:text="取消" />

    </LinearLayout>


</LinearLayout>
